{
  "name": "gpt-telegram-bot",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "engines": {
    "node": ">=16",
    "yarn": ">=1 <2",
    "npm": ">=8 <9"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "NODE_ENV=production node build/index.js",
    "dev": "yarn build && concurrently \"npx tsc --watch\" \"cross-env NODE_ENV=development nodemon -q build/index.js\"",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "gpt",
    "chat-gpt",
    "telegram-bot",
    "typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "Oleg Kireev",
    "email": "flucky@mail.ru"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/fluent-ffmpeg": "^2.1.21",
    "axios": "^1.4.0",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "husky": "^8.0.3",
    "openai": "^3.2.1",
    "telegraf": "^4.12.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@ubic/eslint-config": "5.1.0-alpha.9",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "typescript": "^4.9.4"
  }
}
